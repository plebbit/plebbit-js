    "knex": "1.0.7",
    "libp2p-crypto": "0.21.2",
    "node-fetch": "2.6.7",
    "node-forge": "1.3.1",
    "path-browserify": "^1.0.1",
    "process": "^0.11.10",
    "uint8arrays": "3.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/core": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "@webpack-cli/generators": "^2.4.2",
    "babel-loader": "^8.2.5",
    "mocha": "9.2.2",
    "webpack": "^5.72.0",
    "webpack-cli": "^4.9.2",
    "webpack-dev-server": "^4.9.0"
  },
  "type": "module"
