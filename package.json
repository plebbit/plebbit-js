{
  "name": "@plebbit/plebbit-js",
  "version": "0.0.3",
  "description": "plebbit.js will be an NPM module to wrap around the IPFS APIs used by Plebbit. It will be used in all clients: CLI, Electron (Desktop GUI) and Web.",
  "exports": {
    ".": {
      "browser": "./dist/browser/index.js",
      "import": "./dist/node/esm/index.js",
      "require": "./dist/node/cjs/index.js"
    },
    "./package.json": "./package.json"
  },
  "main": "./dist/node/cjs/index.js",
  "browser": "./dist/browser/index.js",
  "scripts": {
    "test": "mocha --timeout 0",
    "build:cjs": "babel src --out-dir dist/node/cjs --config-file ./config/babel-cjs.js && echo '{\"type\": \"commonjs\"}'> dist/node/cjs/package.json",
    "build:esm": "babel src --out-dir dist/node/esm --config-file ./config/babel-esm.js && echo '{\"type\": \"module\"}'> dist/node/esm/package.json",
    "build:browser": "TODO and also add browser: ./dist/browser/index.js to package.json",
    "build": "npm run build:cjs && npm run build:esm"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/plebbit/plebbit-js.git"
  },
  "keywords": [
    "plebbit"
  ],
  "author": "Esteban Abaroa, Rinse",
  "license": "GPL-2.0-only",
  "bugs": {
    "url": "https://github.com/plebbit/plebbit-js/issues"
  },
  "homepage": "https://github.com/plebbit/plebbit-js#readme",
  "dependencies": {
    "better-sqlite3": "7.5.1",
    "captcha-canvas": "3.1.0",
    "cborg": "1.9.0",
    "debug": "4.3.4",
    "form-data": "4.0.0",
    "ipfs-core": "0.14.2",
    "ipfs-http-client": "56.0.2",
    "it-all": "1.0.6",
    "it-last": "1.0.6",
    "jose": "4.6.1",
    "js-sha256": "0.9.0",
    "knex": "1.0.7",
    "libp2p-crypto": "0.21.2",
    "node-fetch": "2.6.7",
    "node-forge": "1.3.1",
    "uint8arrays": "3.0.0",
    "uuid": "8.3.2"
  },
  "devDependencies": {
    "@babel/cli": "^7.17.10",
    "@babel/preset-env": "^7.17.10",
    "babel-plugin-add-module-exports": "^1.0.4",
    "mocha": "9.2.2"
  },
  "type": "module"
}
