{
    "version": "0.2.0",
    "configurations": [
        {
            "type": "node",
            "request": "launch",
            "runtimeVersion": "20",
            "name": "Launch current file with NodeJS",
            "skipFiles": ["<node_internals>/**"],
            "program": "${file}",
            "console": "integratedTerminal",
            "env": {
                "DEBUG": "plebbit*,libp2p*, helia*",
                "USE_RPC": "1"
            }
        },
        {
            "type": "node",
            "request": "launch",
            "runtimeVersion": "20",
            "name": "Launch Test Server",
            "skipFiles": ["<node_internals>/**"],
            "program": "${workspaceFolder}/test/server/test-server.js",
            "console": "integratedTerminal",
            "env": {
                "DEBUG": "plebbit-js:*, -plebbit*trace",
                "REMOTE_PLEBBIT_CONFIGS": "remote-kubo-rpc",
                "START_RPC_SERVER": "1"
            }
        },
        {
            "args": [
                "run",
                "--config",
                "${workspaceFolder}/config/vitest.launch.config.js",
                "--bail",
                "1",
                "${workspaceFolder}/rpc/test/node-and-browser/plebbit-ws-server.test.js"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Test RPC SERVER",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
            "request": "launch",
            "skipFiles": ["<node_internals>/**"],
            "type": "node",
            "env": {
                "DEBUG": "plebbit-js:*, -plebbit*trace"
            }
        },
        {
            "args": [
                "run",
                "--config",
                "${workspaceFolder}/config/vitest.launch.config.js",
                "--bail",
                "0",
                "${file}"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Debug current file (remote)",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
            "request": "launch",
            "outFiles": [],
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "runtimeArgs": ["--max-old-space-size=18192"],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
                "${workspaceFolder}/dist/browser/**",
            ],
            "type": "node",
            "env": {
                "DEBUG": "plebbit*, -plebbit*trace",
                "PLEBBIT_CONFIGS": "remote-ipfs-gateway,remote-kubo-rpc,remote-libp2pjs"
            }
        },
        {
            "args": [
                "run",
                "--config",
                "${workspaceFolder}/config/vitest.launch.config.js",
                "--bail",
                "1",
                "${file}"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Debug current file (local)",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
            "request": "launch",
            "outFiles": [],
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "runtimeArgs": ["--max-old-space-size=18192"],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
                "${workspaceFolder}/dist/browser/**",
            ],
            "type": "node",
            "env": {
                "DEBUG": "plebbit-js:*, -plebbit*trace",
                "PLEBBIT_CONFIGS": "local-kubo-rpc"
            }
        },
        {
            "args": [
                "run",
                "--config",
                "${workspaceFolder}/config/vitest.launch.config.js",
                "--bail",
                "1",
                "${file}"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "(RPC) - Debug current file",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
            "request": "launch",
            "outFiles": [],
            "runtimeArgs": ["--max-old-space-size=18192"],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
                "${workspaceFolder}/dist/browser/**",
            ],
            "type": "node",
            "env": {
                "DEBUG": "plebbit-js:*",
                "PLEBBIT_CONFIGS": "remote-plebbit-rpc",
                "USE_RPC": "1"
            }
        },
        {
            "args": [
                "run",
                "--config",
                "${workspaceFolder}/config/vitest.launch.config.js",
                "--fileParallelism",
                "--maxWorkers",
                "4",
                "--bail",
                "1",
                "${workspaceFolder}/test/node-and-browser"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Node and browser tests (Parallel)",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
            "request": "launch",
            "outFiles": [],
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "runtimeArgs": ["--max-old-space-size=18192", "--trace-warnings", "--unhandled-rejections=strict"],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
                "${workspaceFolder}/dist/browser/**",
            ],
            "type": "node",
            "env": {
                "DEBUG": "plebbit-js:*, -plebbit-js*trace",
                "PLEBBIT_CONFIGS": "remote-libp2pjs"
            }
        },
        {
            "args": [
                "run",
                "--config",
                "${workspaceFolder}/config/vitest.launch.config.js",
                "--bail",
                "1",
                "${workspaceFolder}/test/node-and-browser"
            ],
            "internalConsoleOptions": "openOnSessionStart",
            "name": "Node and browser tests (Sequential)",
            "console": "integratedTerminal",
            "program": "${workspaceFolder}/node_modules/vitest/vitest.mjs",
            "request": "launch",
            "outFiles": [],
            "resolveSourceMapLocations": ["${workspaceFolder}/**", "!**/node_modules/**"],
            "runtimeArgs": ["--max-old-space-size=18192", "--trace-warnings", "--unhandled-rejections=strict"],
            "skipFiles": [
                "<node_internals>/**",
                "**/node_modules/**",
                "${workspaceFolder}/dist/browser/**",
            ],
            "type": "node",
            "env": {
                "DEBUG": "*",
                "PLEBBIT_CONFIGS": "remote-libp2pjs"
            }
        }
    ]
}
